{
    "name": "laravie/api",
    "description": "A RESTful API package for the Laravel and Lumen frameworks.",
    "keywords": ["api", "dingo", "laravel", "restful"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Lewis",
            "email": "jason.lewis1991@gmail.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dingo\\Api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Api\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.5.9 || ^5.6.0 || ^7.0",
        "illuminate/routing": "~5.4.0",
        "illuminate/support": "~5.4.0",
        "laravie/blueprint": "^0.2",
        "league/fractal": ">=0.12.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "~5.4.0",
        "mockery/mockery": "~0.9.4",
        "orchestra/testbench": "~3.4.0",
        "phpunit/phpunit": "^5.7",
        "tymon/jwt-auth": "dev-develop"
    },
    "replace": {
        "dingo/api": "*"
    },
    "suggest": {
        "tymon/jwt-auth": "Protect your API with JSON Web Tokens."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
