{
    "name": "laravie/api",
    "description": "A RESTful API package for the Laravel and Lumen frameworks.",
    "keywords": ["api", "dingo", "laravel", "restful"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Lewis",
            "email": "jason.lewis1991@gmail.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dingo\\Api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Api\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "illuminate/routing": "~5.8.0 || ~5.9.0",
        "illuminate/support": "~5.8.0 || ~5.9.0",
        "laravie/blueprint": "^1.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "~5.8.0 || ~5.9.0",
        "league/fractal": "^0.17",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.8.0 || ~3.9.0",
        "tymon/jwt-auth": "^1.0"
    },
    "replace": {
        "dingo/api": "*"
    },
    "suggest": {
        "league/fractal": "Transform response using Fractal (^0.17).",
        "tymon/jwt-auth": "Protect your API with JSON Web Tokens (^2.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
